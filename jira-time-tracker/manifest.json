{
  "name": "Jira time tracker",
  "description": "Help jira time tracking easier",
  "version": "0.0.0.1",
  "manifest_version": 3,
  "permissions": [
    "storage"
  ],
  "action": {
    "default_popup": "popup.html",
    "default_title": "Open the popup"
  },
  "icons": {
    "16": "logo.png",
    "48": "logo.png",
    "128": "logo.png"
  },
  "host_permissions": [
    "https://*/"
  ]
}